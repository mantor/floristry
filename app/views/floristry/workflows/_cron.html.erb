<div class="rat-cron">
  Cron "<%= cron.params.keys[0] %>"
  <%- floCron.each_with_index do |child, index| %>
    <%= render :partial => child.class.to_s.underscore,
               :layout => child.layout,
               :object => child,
               :locals => {:leaf => child},
               :as => child.as_variable_name
    %>
    <%= render :partial => 'floristry/sequence_spacer' unless index == floCron.size - 1 %>
  <%- end %>
</div>